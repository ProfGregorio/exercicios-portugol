programa {
	/*
		Faça um algoritmo que leia três números e mostre a multiplicação desses números.
	*/
  funcao inicio() {
  inteiro produto, num1, num2, num3 

	escreva("Digite o valor do primeiro número: ") 
	leia(num1) 
	escreva("Digite o valor do segundo número: ") 
	leia(num2) 
	escreva("Digite o valor do terceiro número: ") 
	leia(num3) 
	
	produto = num1 * num2 * num3 
	
  escreva("O produto dos números é: ", produto)
  }
}
